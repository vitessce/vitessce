    "babel-preset-react-app": "^9.1.2",
    "clsx": "^1.1.1",
    "copy-text-to-clipboard": "^2.2.0",
    "docusaurus-plugin-sass": "^0.2.1",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.28.0",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "prism-react-renderer": "^1.1.1",
    "query-string": "^6.13.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.2.4",
