{
  "name": "vitessce",
  "version": "2.0.1",
  "description": "Vitessce app and React component library",
  "author": "Gehlenborg Lab",
  "homepage": "http://vitessce.io/",
  "repository": "https://github.com/vitessce/vitessce/",
  "main": "index.js",
  "scripts": {
    "start": "pnpm -r --parallel start",
    "start-demo": "pnpm -r --parallel --filter=!docs start",
    "start-docs": "pnpm -r --parallel --filter=!demo start",
    "build:ci": "pnpm -r --workspace-concurrency 1 build",
    "build": "pnpm -r build",
    "test": "pnpm -r --workspace-concurrency 1 test",
    "lint": "eslint \"packages/**/src/**/*.js\" \"sites/*/src/**/*.js\" \"examples/*/src/**/*.js\"",
    "lint-fix": "eslint --fix \"packages/**/src/**/*.js\" \"sites/*/src/**/*.js\" \"examples/*/src/**/*.js\"",
    "clean": "pnpm -r exec -- rm -rf node_modules",
    "meta-update": "cross-env META_UPDATER_MODE=write pnpm meta-updater",
    "meta-dryrun": "cross-env META_UPDATER_MODE=dryrun pnpm meta-updater",
    "meta-version-only": "cross-env META_UPDATER_MODE=versiononly pnpm meta-updater"
  },
  "devDependencies": {
    "@pnpm/meta-updater": "^0.0.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.16.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-canvas-mock": "^2.3.0",
    "jsdom": "^20.0.0",
    "jsdom-worker": "^0.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.55.0",
    "serve-static": "^1.15.0",
    "terser": "^5.15.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.23.4",
    "cross-env": "^7.0.0",
    "json-diff-ts": "^1.2.5",
    "lodash": "^4.17.21"
  },
  "pnpm": {
    "overrides": {
      "zarr": "0.5.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    }
  },
  "keywords": [
    "react-component"
  ],
  "license": "MIT"
}
