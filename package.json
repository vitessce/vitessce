{
  "name": "vitessce-monorepo",
  "private": true,
  "version": "3.6.15",
  "description": "Vitessce app and React component library",
  "author": "HIDIVE Lab at HMS",
  "homepage": "http://vitessce.io/",
  "repository": "https://github.com/vitessce/vitessce/",
  "scripts": {
    "start": "pnpm -r --parallel start",
    "start-demo": "pnpm -r --parallel --filter=!docs start",
    "start-docs": "pnpm -r --parallel --filter=!demo start",
    "start-html": "pnpm -r start-html",
    "clean-start": "pnpm install && pnpm run clean && pnpm run build && pnpm run start-demo",
    "build:ci": "pnpm -r --workspace-concurrency 1 build",
    "build": "pnpm -r build",
    "build-json-schema": "pnpm -C packages/json-schema build-json-schema",
    "start-tsc": "tsc -b --watch",
    "build-tsc": "tsc --build",
    "bundle:ci": "pnpm -r --workspace-concurrency 1 bundle",
    "bundle": "pnpm -r bundle",
    "test": "vitest --run",
    "test-ui": "vitest --ui",
    "test-watch": "vitest --watch",
    "lint": "eslint \"packages/**/src/**/*.{js,jsx,ts,tsx}\" \"sites/*/src/**/*.{js,jsx,ts,tsx}\" \"examples/*/src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint --fix \"packages/**/src/**/*.{js,jsx,ts,tsx}\" \"sites/*/src/**/*.{js,jsx,ts,tsx}\" \"examples/*/src/**/*.{js,jsx,ts,tsx}\"",
    "clean": "pnpm -r exec -- rm -rf dist dist-tsc tsconfig.tsbuildinfo",
    "clean-deps": "pnpm -r exec -- rm -rf node_modules",
    "clean-all": "pnpm run clean && pnpm run clean-deps",
    "publint": "pnpm -r --filter \"./packages/**\" exec publint",
    "changeset-status": "changeset status --since=origin/main",
    "check-makestyles-keys": "pnpm exec sg scan --json | node ./scripts/check-makestyles-keys.mjs",
    "create-view": "node ./scripts/create-view.mjs",
    "sync-cdn": "node ./scripts/sync-cdn.mjs cdn"
  },
  "devDependencies": {
    "@ast-grep/cli": "^0.6.3",
    "@ast-grep/napi": "0.38.1",
    "@changesets/cli": "^2.17.0",
    "@changesets/get-github-info": "^0.5.2",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/d3-array": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "cross-env": "^7.0.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-require-extensions": "^0.1.3",
    "gray-matter": "^4.0.3",
    "isomorphic-git": "^1.24.5",
    "jest-canvas-mock": "2.5.2",
    "jsdom": "^26.1.0",
    "jsdom-worker": "^0.3.0",
    "json-diff-ts": "^1.2.5",
    "lodash-es": "catalog:",
    "mdast-util-to-string": "^1.0.6",
    "publint": "^0.1.11",
    "react": "catalog:",
    "react-dom": "catalog:",
    "remark-parse": "^7.0.1",
    "remark-stringify": "^7.0.3",
    "sass": "1.55.0",
    "serve-static": "^1.15.0",
    "set.prototype.intersection": "^1.1.5",
    "svg-inline-loader": "^0.8.2",
    "terser": "^5.40.0",
    "typescript": "^5.5.3",
    "unified": "^8.3.2",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vitest-canvas-mock": "^0.3.3"
  },
  "pnpm": {
    "overrides": {
      "zarr": "0.5.1",
      "jest-canvas-mock": "2.5.0"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19",
        "react-dom": "19"
      }
    },
    "patchedDependencies": {
      "@janelia-flyem/neuroglancer@2.37.5": "patches/@janelia-flyem__neuroglancer@2.37.5.patch",
      "@janelia-flyem/react-neuroglancer@2.5.0": "patches/@janelia-flyem__react-neuroglancer@2.5.0.patch",
      "rc-tree@2.1.0": "patches/rc-tree@2.1.0.patch"
    }
  },
  "license": "MIT",
  "packageManager": "pnpm@9.5.0"
}