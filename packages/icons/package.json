{
  "name": "@vitessce/icons",
  "version": "2.0.2",
  "author": "Gehlenborg Lab",
  "homepage": "http://vitessce.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitessce/vitessce.git"
  },
  "license": "MIT",
  "main": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist/ && rimraf *.tsbuildinfo && tsc && pnpm run build-lib",
    "build-lib": "pnpm run build-lib:prod && pnpm run build-lib:dev",
    "build-lib:prod": "cross-env APP_ENV=production vite build --mode lib",
    "build-lib:dev": "cross-env APP_ENV=development vite build --mode lib"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.0.0-beta.0",
    "cross-env": "^7.0.0",
    "rimraf": "^3.0.2",
    "vite": "^3.0.0",
    "vite-plugin-svgr": "^2.2.1"
  }
}
