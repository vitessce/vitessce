  },
  "license": "MIT",
  "type": "module",
  "main": "dist-tsc/index.js",
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.js",
    "exports": {
      ".": {
        "types": "./dist-tsc/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "files": [
    "src",
    "dist",
