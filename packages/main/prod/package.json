{
  "name": "vitessce",
  "version": "2.0.0-beta.1",
  "author": "Gehlenborg Lab",
  "homepage": "http://vitessce.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitessce/vitessce.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.min.mjs",
    "browser": "dist/index.min.umd.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "rimraf ./dist/ && rimraf *.tsbuildinfo && tsc && pnpm run build-lib",
    "build-lib": "pnpm run build-lib:prod",
    "build-lib:prod": "cross-env APP_ENV=production vite build --mode lib",
    "build-lib:dev": "cross-env APP_ENV=development vite build --mode lib"
  },
  "dependencies": {
    "@vitessce/all": "workspace:*",
    "@vitessce/config": "workspace:*",
    "@vitessce/constants": "workspace:*",
    "@vitessce/export-utils": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.3.2",
    "cross-env": "^7.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "vite": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
